<h6>Info Labels (These labels will be shown in the popup , please see in the right for pop up preview)</h6>
<div class="row-fluid">
    <fieldset class="form-horizontal span6">
        <div class="control-group">
            <label class="control-label">Homesite</label>
            <div class="controls">
                <input type="text" id="lotHomesite" data-bind="value:ISP2_POPUP_HOMESITE" placeholder="Homesite" maxlength="18"/>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">Lot Size</label>
            <div class="controls">
                <input type="text" id="lotSize" data-bind="value:ISP2_POPUP_LOTSIZE" placeholder="Size" maxlength="25"/>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">Lot Premium Price</label>
            <div class="controls">
                <input type="text" id="lotPremiumPrice" data-bind="value:ISP2_POPUP_PRMPRICE" placeholder="Premium Price" maxlength="25"/>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">Block</label>
            <div class="controls">
                <input type="text" id="lotBlock" data-bind="value:ISP2_POPUP_BLOCK" placeholder="Block" maxlength="25"/>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">Phase</label>
            <div class="controls">
                <input type="text" id="lotPhase" data-bind="value:ISP2_POPUP_PHASE" placeholder="Phase" maxlength="25"/>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">Swing</label>
            <div class="controls">
                <input type="text" id="lotSwing" data-bind="value:ISP2_POPUP_SWING" placeholder="Swing" maxlength="25"/>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">Elevation</label>
            <div class="controls">
                <input type="text" id="lotElevation" data-bind="value:ISP2_POPUP_ELEVATION" placeholder="Elevation" maxlength="25"/>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">Address</label>
            <div class="controls">
                <input type="text" id="lotAddress" data-bind="value:ISP2_POPUP_ADDRESS" placeholder="Address" maxlength="25" />
            </div>
        </div>
    </fieldset>

    <div class="modal-static span5">

        <div class="lot-name" id="Homesite">Homesite 31 / Available</div>

        <div class="otherInfogroup">
            <span class="otherInfo" id="lotsize"><strong>Size:</strong></span><span class="othrVal">4000</span>
            <span class="otherInfo" id="lotblock"><strong>Block:</strong></span><span class="othrVal">B</span>
            <span class="otherInfo" id="lotphase"><strong>Phase:</strong></span><span class="othrVal">P</span>
            <span class="otherInfo" id="lotswing"><strong>Swing:</strong></span><span class="othrVal">Left</span>
            <span class="otherInfo" id="lotelevation"><strong>Elevation:</strong></span><span class="othrVal">3</span>
            <div class="otherInfo addr breakAll" id="lotpremium"><strong>Premium Price: </strong><span>$900</span></div>
            <div class="otherInfo addr breakAll" id="lotaddress"><strong>Address: </strong><span>6632 Starcross Drive</span></div>
        </div>

        <h3>Available Plans in this Community</h3>
        <ul class="plansul">
            <li>The Manor</li>
            <li>The Hampton</li>
        </ul>

        <p style="font-size:14px;">Front of house faces West. Large corner plot, close to pool and clubhouse.</p>

    </div>

</div>
<div class="controls" style="color:gray;font-size:11px;">(Max character length: 25, For Homesite: 18)</div>
<hr class="style1">


@using BHI.SalesArchitect.WebAdmin.Lib.Extensions
@using System.Web.Optimization
@using RazorEngine.Text;
@model BHI.SalesArchitect.WebAdmin.Models.SiteMapPreview
@{
    Layout = null;
}
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <meta name="viewport" content="initial-scale=1.0, maximum-scale=6.0" />
    <script type="text/javascript" src="~/Scripts/jquery-1.10.2.min.js"></script>
    <link href="/Content/preview.css" rel="Stylesheet" type="text/css" />
    <link href="/Content/simple-slider.css" rel="stylesheet" type="text/css" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-migrate/1.2.1/jquery-migrate.js"></script>
    <script type="text/javascript" src="~/Scripts/svg/jquery.svg.min.js"></script>
    <script type="text/javascript" src="~/Scripts/ios/MasterSitePlan.js"></script>
    <script type="text/javascript" src="~/Scripts/ios/Tap.js"></script>
    <script src="/Scripts/fastclick.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Scripts/simple-slider.js"></script>
    <style>
        svg {
            -webkit-tap-highlight-color: rgba(0,0,0,0);
        }
    </style>
</head>
<body>
    <div class="main-content-page">
        <div class="map-area-div">
            <div class="map-area-table-cell">
                <div id="mapData" style="display: none">
                </div>
                <div id="canvas" class="svg-div">
                    @(new RawString(Model.MapData))
                </div>
            </div>
        </div>
    </div>
    <script type="text/javascript">
        var masterPlan = querystring('mastersiteid').length > 0 || '@Model.IsMasterSitePlan' == 'true' || '@Model.IsMasterSitePlan' == 'True';
        $(function () {
            //setupSiteMapPreview('@(new RawString(System.Web.Helpers.Json.Encode(Model.LotData)))',@Model.PartnerID);
        });
        $(document).ready(function(){
            //    initReadyFunctions();
            FastClick.attach(document.body);
            setupSiteMapPreview('@(new RawString(System.Web.Helpers.Json.Encode(Model.LotData)))',@Model.PartnerID);
        });
    </script>
</body>
</html>

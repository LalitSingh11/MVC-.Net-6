<h6>Other Titles</h6>
<fieldset class="form-horizontal">
    <div class="control-group" style="margin-bottom:0px !important;">
        <label class="control-label">Lot Status Color Key for Geospatial ISP</label>
        <div class="controls">
            <input type="text" name="lotStatusKey" data-bind="value:ISP3_LOTSTATUSKEY" placeholder="Lot Status Color Key" maxlength="22" />
        </div>
    </div>
    <div class="controls" style="color: gray; font-size: 11px; margin-left: 0px; margin-bottom: 10px;">(Max character length: 22)</div>
    <div class="control-group" style="margin-bottom:0px !important;">
        <label class="control-label">Default Title for Interactive Media</label>
        <div class="controls">
            <input type="text" name="intMediaTitle" data-bind="value:ISP_INTMEDIATITLE" placeholder="View Media" maxlength="100" />
        </div>
    </div>
    <div class="controls" style="color: gray; font-size: 11px; margin-left: 0px; margin-bottom: 10px;">(Max character length: 100)</div>
</fieldset>
<div class="form-actions">
    <input type="button" class="btn btn-primary " value="Save Changes" data-bind="click:savePopupConfiguration" />
    <input type="button" class="btn " value="Cancel" data-bind="click:getPopupConfiguration" />
</div>
